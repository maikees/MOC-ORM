{
  "name": "moc-solucoes/moc-orm",
  "type": "library",
  "description": "Orm for PHP >= 5.3.",
  "keywords": [
    "orm",
    "php",
    "orm-php",
    "moc",
    "mysql",
    "postgres"
  ],
  "homepage": "https://github.com/maikees/moc-orm",
  "license": "MIT",
  "authors": [
    {
      "name": "MOC Soluções",
      "email": "contato@mocsolucoes.com.br",
      "homepage": "http://mocsolucoes.com.br",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "phpunit/phpunit-mock-objects": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "MocOrm\\Connection\\": "lib/",
      "MocOrm\\Model\\": "lib/",
      "MocOrm\\Support\\": "lib/support/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MocOrm\\Tests\\": "tests/",
      "MocOrm\\Tests\\Connection\\": "tests/Connection/",
      "MocOrm\\Tests\\Model\\": "tests/Model/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "test-cover": "phpunit --coverage-text"
  }
}
